{
  "expo": {
    "name": "Song Share",
    "slug": "SongShare",
    "scheme": "songshare",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "spotify"
        ]
      }
    },
    "android": {
      "package": "com.jasonmillen.AwesomeProject",
      "versionCode": 2
    },
    "userInterfaceStyle": "automatic"
  }
}
